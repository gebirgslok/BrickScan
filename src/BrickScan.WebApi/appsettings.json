{
  "Serilog": {
    "Using": [ "Serilog.Sinks.File" ],
    "MinimumLevel": {
      "Default": "Error",
      "Override": {
        "Microsoft": "Error",
        "System": "Error"
      }
    },
    "Enrich": [ "FromLogContext", "WithMachineName", "WithProcessId", "WithThreadId" ],
    "WriteTo": [
      {
        "Name": "File",
        "Args": {
          "path": "D:\\Home\\LogFiles\\Application\\log.txt",
          "outputTemplate": "{Timestamp:G} [{Level:u3}] {Application} {MachineName} {ThreadId} [{RequestId}] {SourceContext}: {Message}{NewLine:1}{Exception:1}",
          "fileSizeLimitBytes": 52428800, //50 MB
          "retainedFileCountLimit": 3,
          "rollingInterval": "Day"
        }
      }
    ]
  },
  "AllowedHosts": "*",
  "MaxPredictionMatches": 10,
  "ModelFilePath": "D:\\Home\\Model\\MlModel.zip"
}
